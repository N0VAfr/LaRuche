            {% if markers is defined and markers is iterable and markers|length > 0 %}
                <div class="marker-container">
                    {% for marker in markers %}
                        {% set id = marker.id %}
                        {% set region_id = marker.region_id %}
                        {% set xCoord = marker.x_coord %}
                        {% set yCoord = marker.y_coord %}
                        {% set name = marker.name %}
                        {% set url = marker.url %}

                        {# Assuming (0,0) is at the top left, adjust yCoord for the top position #}
                        {% set marginLeft = xCoord ~ '%' %}
                        {% set topPosition = yCoord ~ '%' %}

                        <a class="Marker" href="{{ url }}" style="margin-left: {{ marginLeft }}; position: absolute; top: {{ topPosition }};">
                            <img class="Marker" src="{{ asset('images/pin.png') }}" alt="Pin Image">
                        </a>
                    {% endfor %}
                </div>
            {% endif %}





